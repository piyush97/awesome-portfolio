{
  "name": "awesome-portfolio-server",
  "version": "1.0.0",
  "description": "Server for awesome Portfolio",
  "main": "index.js",
  "repository": "https://github.com/piyush97/awesome-portfolio",
  "author": "piyush mehta",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@piyush97/typescript-node": "^1.1.3",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.13.7"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "nodemon": "^2.0.12"
  },
  "scripts": {
    "watch": "ts-node-dev --respawn index.dev.ts",
    "format": "prettier --write .",
    "dev": "tsnd --respawn --files -r @babel/register --inspect -- index.dev.ts",
    "build": "babel ./src -d ./dist --extensions .ts --copy-files",
    "typecheck": "tsc",
    "start": "node dist/index.js",
    "lint:base": "eslint --ext .js,.json,.ts",
    "lint": "yarn lint:base src"
  }
}
